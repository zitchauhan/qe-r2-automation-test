{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "pickupResponse": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "storeId": {
              "type": "string"
            },
            "skus": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "skuId": {
                      "type": "string"
                    },
                    "availableQuantity": {
                      "type": "string"
                    },
                    "inventoryStatus": {
                      "type": "string"
                    },
                    "availabilityDate": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "skuId",
                    "availableQuantity",
                    "inventoryStatus",
                    "availabilityDate"
                  ]
                }
              ]
            }
          },
          "required": [
            "storeId",
            "skus"
          ]
        }
      ]
    }
  },
  "required": [
    "pickupResponse"
  ]
}