{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "profile": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "receiveEmailPreference": {
          "type": "boolean"         
        },
        "passwordExpiredFlag": {
          "type": "boolean"
        }
      },
      "required": [
        "firstName",
        "lastName",
        "email",
        "receiveEmailPreference",
        "passwordExpiredFlag"
      ]
    }
  },
  "required": [
    "profile"
  ]
}